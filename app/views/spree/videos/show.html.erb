<%= link_to 'Back', videos_path %>

<p id="notice"><%= notice %></p>

<%= image_tag(@video.youtube_data.thumbnail_url) %>

<p>
  <b>Title:</b>
  <%= @video.youtube_data.title %>
</p>

<p>
  <b>Published:</b>
  <%= @video.youtube_data.published_at %>
</p>

<p>
  <b>Description:</b>
  <%= @video.youtube_data.description %>
</p>

<p>
  <%= raw @video.youtube_data.embed_html %>
</p>